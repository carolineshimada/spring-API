spring.datasource.url=jdbc:sqlserver://localhost;databaseName=AuroraDelivery;integratedSecurity=true
spring.datasource.driver-class-name=com.microsoft.sqlserver.jdbc.SQLServerDriver

# Localização da DLL de autenticação
# Isso deve ser configurado como uma variável de sistema ou adicionando uma linha de comando JVM args
# Exemplo: -Djava.library.path=C:\sqljdbc

spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.SQLServerDialect
